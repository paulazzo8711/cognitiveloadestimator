{
  "name": "cognitiveloadestimator",
  "displayName": "CognitiveLoadEstimator",
  "description": "Utilize eye tracker data to determine cognitive load and give the user suggestions to reduce cognitive load.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Cognitive Load Estimator",
      "properties": {
        "cognitiveloadestimator.directoryPath": {
          "type": "string",
          "default": "",
          "description": "Path to the directory containing Excel files for cognitive load estimation."
        }
      }
    },
    "commands": [
      {
        "command": "cognitiveloadestimator.uploadFile",
        "title": "Upload File and Predict Cognitive Load"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "form-data": "^4.0.0"
  }
}
